{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "better-docs/category"
    ],
    "templates": {
        "better-docs": {
            "name": "Interactive Visualization for Information Security Analysis: JavaScript Documentation"
        },
        "resources": {
            "svgdotjs": "https://github.com/svgdotjs/svg.js"
        }
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "recurse": true,
        "verbose": true,
        "tutorials": "./examples",
        "template": "node_modules/better-docs/"
    }
}