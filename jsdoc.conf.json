{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "better-docs/category"
    ],
    "templates": {
        "better-docs": {
            "name": "Interactive Visualization for Information Security Analysis: JavaScript Documentation",
            "css": "styles/custom.css",
            "hideGenerator": true,
            "navigation": [
                {
                    "label": "Gitlab",
                    "href": "https://git.uibk.ac.at/csat2206/Bachelorarbeit"
                }
            ]
        },
        "resources": {
            "svgdotjs": "https://github.com/svgdotjs/svg.js"
        },
        "search": true
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "recurse": true,
        "verbose": true,
        "tutorials": "./examples",
        "readme": "./readme.md",
        "template": "node_modules/better-docs/"
    }
}