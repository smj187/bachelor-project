{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "better-docs/component",
        "plugins/summarize"
    ],
    "templates": {
        "better-docs": {
            "name": "Graph visualization"
        },
        "resources": {
            "svgdotjs": "https://github.com/svgdotjs/svg.js"
        }
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/toast-jsdoc"
    }
}