  },
  "scripts": {
    "rest-server": "node ./public/backend/server.js",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel live-server watch rest-server",
    "live-server": "npx live-server --port=5500 --open=public/index.html",
    "docs": "jsdoc -c jsdoc.conf.json",
    "lint": "npx eslint ./src --fix",
    "docss": "doxdox 'lib/**/*.js' --layout markdown --output DOCUMENTATION.md",
    "test": "./node_modules/.bin/cypress open",
    "test1": "npm-run-all --parallel test:server test",
    "test:server": "npx live-server --port=9987 --open=cypress/index.html",
    "asdf": "start-server-and-test start http://localhost:5500 test"
  }
