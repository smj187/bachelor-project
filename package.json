{
  "version": "1.0.2",
  "name": "interactive-visualization-for-information-security-analysis",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.8.3",
    "@cypress/webpack-preprocessor": "^5.1.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "better-docs": "^2.0.1",
    "body-parser": "^1.19.0",
    "clamp-js": "^0.7.0",
    "cors": "^2.8.5",
    "cypress": "^4.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "express": "^4.17.1",
    "jsdoc": "^3.6.3",
    "jsdoc-template": "^1.2.0",
    "jsdoc-to-markdown": "^5.0.3",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "rollup": "^2.10.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-url": "^3.0.1",
    "svg-intersections": "^0.4.0",
    "svg-url-loader": "^5.0.0",
    "webpack": "^4.43.0"
  },
  "dependencies": {
    "@svgdotjs/svg.draggable.js": "^3.0.2",
    "@svgdotjs/svg.filter.js": "^3.0.7",
    "@svgdotjs/svg.js": "^3.0.16"
  },
  "scripts": {
    "rest-server": "node ./public/backend/server.js",
    "dev": "rollup -c -w --environment BUILD:dev",
    "live-server": "npx live-server --port=5500 --open=public/index.html",
    "docs": "jsdoc -c jsdoc.conf.json",
    "lint": "npx eslint ./src --fix",
    "test": "npm-run-all --parallel test:server test:integration",
    "test:integration": "npx cypress open",
    "test:server": "npx live-server --port=9987 --entry-file=public/test/index.html --no-browser",
    "build:esm": "rollup -c --environment BUILD:ESM",
    "build:umd": "rollup -c --environment BUILD:UMD",
    "build:prod": "run esm und umd gliechzeitig",
    "grid-liveserver": "npx live-server --port=5501 --open=public/demos/layouts/grid-example.html",
    "contextual-liveserver": "npx live-server --port=5501 --open=public/demos/layouts/contextual-example.html",
    "radial-liveserver": "npx live-server --port=5501 --open=public/demos/layouts/radial-example.html",
    "tree-liveserver": "npx live-server --port=5501 --open=public/demos/layouts/tree-example.html",
    "advanced-liveserver": "npx live-server --port=5501 --open=public/demos/layouts/advanced-example.html",
    "demo:grid": "npm-run-all --parallel rest-server grid-liveserver",
    "demo:contextual": "npm-run-all --parallel rest-server contextual-liveserver",
    "demo:radial": "npm-run-all --parallel rest-server radial-liveserver",
    "demo:tree": "npm-run-all --parallel rest-server tree-liveserver",
    "demo:advanced": "npm-run-all --parallel rest-server advanced-liveserver"
  }
}
