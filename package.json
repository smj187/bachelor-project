    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "express": "^4.17.1",
    "jsdoc": "^3.6.3",
    "jsdoc-template": "^1.2.0",
    "jsdoc-to-markdown": "^5.0.3",
    "live-server": "^1.2.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.16.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.0.0",
